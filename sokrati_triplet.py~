def pythongorean_triplet(n):
	count = 0
	for i in range(1,n+1):
		for j in range(i+1,n+1):
			for z in range(j+1,n+1):
				sum = i**2 + j**2
				if sum == z**2:
					count +=1
				elif sum < z**2:
					
				elif sum > z**2:
					print 'sum=',sum
					print 'z**2=',z**2
		
	print count
t = input()
for i in range(t):
	n = input()
	pythongorean_triplet(n)